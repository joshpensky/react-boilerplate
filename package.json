{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A basic react app for future projects and endeavors",
  "private": false,
  "main": "build/index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "lint": "eslint *.js",
    "precommit": "lint-staged",
    "stage": "npm run build && surge build {APP_NAME}-stage.surge.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/joshpensky/react-boilerplate.git"
  },
  "keywords": ["react", "boilerplate"],
  "author": "Josh Pensky",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshpensky/react-boilerplate/issues"
  },
  "homepage": "https://github.com/joshpensky/react-boilerplate#readme",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "styled-components": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.16.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.2.0",
    "imagemin-webpack-plugin": "^2.1.1",
    "lint-staged": "^6.1.0",
    "prettier": "^1.12.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "lint-staged": {
    "*.{js,json,md,html}": ["prettier --write", "git add"]
  }
}
